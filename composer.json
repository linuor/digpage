{
    "name": "linuor/digpage",
    "description": "Sample project of a classic website power by Yii2.0",
    "keywords": ["digpage", "yii2", "sample"],
    "homepage": "http://www.digpage.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/linuor/digpage/issues?state=open",
        "wiki": "https://github.com/linuor/digpage/wiki",
        "wiki": "https://github.com/linuor/digpage/wiki",
        "source": "https://github.com/linuor/digpage"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
